{
  "name": "Sentinel",
  "version": "1.0",
  "description": "Track data changes on various websites",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.min.js", "jquery-ui.js", "mimodal.js", "page.js"],
      "css": ["mimodal.css"]
    }
  ],
  "browser_action": {
  "default_title": "Sentinel",
  "default_popup": "popup.html"
  },
  "icons": {
    "128": "faviconSentinel.png"
  },
  "manifest_version": 2
}
